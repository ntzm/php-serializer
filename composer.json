{
    "name": "ntzm/serializer",
    "type": "library",
    "description": "An implementation of PHP's serialize function in PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Nat Zimmermann",
            "email": "nat@ntzm.io"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "doctrine/coding-standard": "^6.0",
        "phpstan/phpstan-shim": "^0.11.7",
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Ntzm\\Serializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ntzm\\Tests\\Serializer\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
